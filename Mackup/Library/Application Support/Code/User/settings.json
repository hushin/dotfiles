{
  "editor.tabSize": 2,
  "window.zoomLevel": 0,
  "keyboard.dispatch": "keyCode",
  "editor.fontSize": 18,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.wordWrap": "on",
  "workbench.colorTheme": "Dracula Soft",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "window.restoreWindows": "all",
  "explorer.confirmDelete": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.fontFamily": "HackGen35Nerd Console",
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "prettier.jsxSingleQuote": true,
  "prettier.trailingComma": "all",
  "markdown-checkbox.italicWhenChecked": false,
  "workbench.startupEditor": "newUntitledFile",
  "editor.copyWithSyntaxHighlighting": false,
  "zentabs.maximumOpenedTabs": 4,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.rendererType": "dom",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": true
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "terminal.integrated.copyOnSelection": true,
  "editor.fontFamily": "Cica, Menlo, Monaco, 'Courier New', monospace",
  "markdown-checkbox.strikeThroughWhenChecked": false,
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "editor.suggestSelection": "first",
  "git.autofetch": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "extensions.ignoreRecommendations": true,
  "code-runner.runInTerminal": true,
  "explorer.confirmDragAndDrop": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.profiles.linux": {
    "bash (login)": {
      "path": "bash",
      "args": [
        "-l"
      ]
    }
  },
  "terminal.integrated.defaultProfile.linux": "bash (login)",
  "workbench.editor.untitled.hint": "hidden"
}